{
  "name": "connectordb-frontend",
  "version": "0.0.1",
  "description": "The default web app that is included in ConnectorDB",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack",
    "test": "jasmine",
    "build:js": "cross-env NODE_ENV=production webpack -p",
    "build:serviceworker": "sed -i -e \"s/GITHASH/$(git rev-parse HEAD)/g\" ../../bin/app/serviceworker.js",
    "build:pipescript": "cp ./node_modules/pipescript/pipescript.js ../../bin/app/",
    "build:html": "rsync -r ./app ../../bin && npm run build:serviceworker",
    "build:branding": "cp branding/title_logo_light.png ../../bin/app",
    "build": "npm run build:html && npm run build:pipescript && npm run build:js && npm run build:branding",
    "js:watch": "cross-env NODE_ENV=debug webpack -d --watch",
    "html:watch": "watch 'npm run build:html' ./app",
    "dev": "npm run build:branding && npm run build:pipescript && run-p 'js:watch' 'html:watch'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/connectordb/connectordb-frontend"
  },
  "homepage": "https://connectordb.io",
  "bugs": {
    "url": "https://github.com/connectordb/connectordb-frontend/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "bootstrap-daterangepicker": "^2.1.25",
    "chart.js": "^2.6.0",
    "classnames": "^2.2.5",
    "codemirror": "^5.26.0",
    "connectordb": "0.0.11",
    "create-react-class": "^15.5.3",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "es6-promise": "^4.1.0",
    "jquery": "^3.2.1",
    "json-loader": "^0.5.4",
    "leaflet": "^1.0.3",
    "localforage": "^1.5.0",
    "localforage-setitems": "^1.3.1",
    "localforage-startswith": "^1.3.0",
    "material-ui": "^0.18.3",
    "mathjs": "^3.13.3",
    "moment": "^2.18.1",
    "npm-run-all": "^4.0.2",
    "pipescript": "^0.2.7",
    "prop-types": "^15.5.10",
    "qrcode.react": "^0.7.1",
    "react": "^15.5.4",
    "react-bootstrap-daterangepicker": "^3.4.0",
    "react-chartjs-2": "^2.1.0",
    "react-codemirror": "^1.0.0",
    "react-dom": "^15.5.4",
    "react-jsonschema-form": "^0.49.0",
    "react-leaflet": "^1.2.2",
    "react-measure": "^2.0.2",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.4",
    "react-tap-event-plugin": "^2.0.1",
    "react-textarea-autosize": "^5.0.6",
    "reactochart": "spotify/reactochart#0.2.1-AI",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.3",
    "redux-thunk": "^2.2.0",
    "regression": "^1.4.0",
    "style-loader": "^0.18.2",
    "watch": "^1.0.2",
    "webpack": "^2.6.1"
  },
  "devDependencies": {
    "jasmine-es6": "^0.4.0"
  }
}
