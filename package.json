{
  "name": "connectordb-frontend",
  "version": "0.0.1",
  "description": "The default web app that is included in ConnectorDB",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:js": "cross-env NODE_ENV=production webpack -p",
    "build:html": "rsync -r ./app ../../bin",
    "build:branding": "cp branding/title_logo_light.png ../../bin/app",
    "build": "npm run build:html && npm run build:js && npm run build:branding",
    "js:watch": "cross-env NODE_ENV=debug webpack -d --watch",
    "html:watch": "watch 'npm run build:html' ./app",
    "dev": "npm run build:branding && parallelshell 'npm run js:watch' 'npm run html:watch'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/connectordb/connectordb-frontend"
  },
  "homepage": "https://connectordb.github.io",
  "bugs": {
    "url": "https://github.com/connectordb/connectordb-frontend/issues"
  },
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chart.js": "^2.2.0-rc.2",
    "classnames": "^2.2.5",
    "codemirror": "^5.20.2",
    "connectordb": "0.0.8",
    "cross-env": "^3.1.1",
    "css-loader": "^0.25.0",
    "es6-promise": "^4.0.5",
    "json-loader": "^0.5.4",
    "leaflet": "^1.0.1",
    "localforage": "^1.4.2",
    "localforage-setitems": "^1.2.0",
    "localforage-startswith": "^1.2.0",
    "material-ui": "^0.16.1",
    "moment": "^2.15.2",
    "parallelshell": "^2.0.0",
    "pipescript": "^0.2.1",
    "react": "^15.3.0",
    "react-bootstrap-daterangepicker": "^3.1.0",
    "react-chartjs-2": "^1.5.0",
    "react-codemirror": "^0.2.6",
    "react-dom": "^15.1.0",
    "react-jsonschema-form": "^0.40.0",
    "react-leaflet": "^1.0.0-rc.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.4",
    "react-tap-event-plugin": "^1.0.0",
    "react-textarea-autosize": "^4.0.2",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.13.1",
    "watch": "^1.0.1",
    "webpack": "^1.13.3"
  }
}
